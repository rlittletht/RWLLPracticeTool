<?xml version="1.0" encoding="utf-8"?>

<!-- For more information on using web.config transformation visit https://go.microsoft.com/fwlink/?LinkId=125889 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

    <appSettings>
        <add key="ClientId" value="24be322a-bc35-4a13-9b7d-f2ae799707c6" xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
        <add key="redirectUri" value="http://localhost/rwp/"  xdt:Transform="SetAttributes" xdt:Locator="Match(key)"/>
    </appSettings>

    <connectionStrings>
        <add name="dbSchedule"
             connectionString="server=cantorix; initial catalog=db0902;trusted_connection=yes"
             xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
    </connectionStrings>
    <system.serviceModel>
        <bindings xdt:Transform="Replace">
            <basicHttpBinding>
                <binding name="BasicHttpBinding_Practice2" />
                <binding name="BasicHttpBinding_PracticeStream" maxReceivedMessageSize="2147483647"
                         transferMode="Streamed" />
            </basicHttpBinding>
        </bindings>
        <client xdt:Transform="Replace">
            <endpoint address="http://dogmatix/RwpSvc/RwpSvc.svc/soap" binding="basicHttpBinding"
                      bindingConfiguration="BasicHttpBinding_Practice2" contract="RwpSvcLocal.Practice"
                      name="BasicHttpBinding_Practice" />
            <endpoint address="http://dogmatix/RwpSvc/RwpSvc.svc/soapStream"
                      binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_PracticeStream"
                      contract="RwpSvcLocal.Practice" name="BasicHttpBinding_PracticeStream" />
        </client>
    </system.serviceModel>
</configuration>

